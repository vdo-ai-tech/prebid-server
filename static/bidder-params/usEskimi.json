{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "eskimi US Adapter Params",
  "description": "A schema which validates params accepted by the eskimi adapter",
  "type": "object",
  "properties": {
    "placementId": {
      "type": "string",
      "minLength": 1,
      "description": "Placement ID"
    },
    "mediaType": {
      "type": "string",
      "minLength": 1,
      "description": "Media type {video, display}"
    }
  },
  "required": [
    "placementId","mediaType"
  ]
}
